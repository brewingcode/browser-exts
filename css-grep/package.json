{
  "name": "css-grep",
  "version": "1.2.2",
  "description": "Greps a regex through DOM elements specified by a CSS selector",
  "scripts": {
    "dev": "grunt dev",
    "build": "grunt build --prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": "https://github.com/brewingcode/css-grep",
  "manifest": {
    "manifest_version": 2,
    "name": "CSS Grep",
    "browser_action": {
      "default_title": "CSS Grep",
      "default_icon": "inactive.png"
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "coffeescript": "^2.2.3",
    "grunt": "brewingcode/grunt",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-coffee": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-watch": "^1.1.0",
    "load-grunt-tasks": "^4.0.0"
  }
}
